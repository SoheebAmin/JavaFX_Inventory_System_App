<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="JavaFX_Files.ModifyProductController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="cancelButton" layoutX="601.0" layoutY="456.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
        <Label layoutX="44.0" layoutY="24.0" text="Add Product">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <VBox layoutX="71.0" layoutY="71.0" prefHeight="137.0" prefWidth="100.0" spacing="21.0">
            <children>
                <Label text="ID" />
                <Label text="Name" />
                <Label text="Inventory" />
                <Label text="Price/Cost" />
            </children>
        </VBox>
        <VBox layoutX="145.0" layoutY="69.0" prefHeight="138.0" prefWidth="100.0" spacing="12.0">
            <children>
                <TextField fx:id="idText" />
                <TextField fx:id="nameText" />
                <TextField fx:id="inventoryText" />
                <TextField fx:id="priceText" />
            </children>
        </VBox>
        <Label layoutX="145.0" layoutY="227.0" text="Min" />
        <TextField fx:id="minText" layoutX="75.0" layoutY="222.0" prefHeight="26.0" prefWidth="54.0" />
        <Button layoutX="525.0" layoutY="456.0" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save" />
        <Label layoutX="48.0" layoutY="227.0" text="Max" />
        <TextField fx:id="maxText" layoutX="169.0" layoutY="222.0" prefHeight="26.0" prefWidth="54.0" />
        <TextField layoutX="423.0" layoutY="24.0" prefHeight="26.0" prefWidth="160.0" promptText="Search by Part ID or Name" />
        <TableView layoutX="319.0" layoutY="69.0" prefHeight="138.0" prefWidth="390.0">
            <columns>
                <TableColumn prefWidth="75.0" text="Part ID" />
                <TableColumn prefWidth="75.0" text="Part Name" />
                <TableColumn prefWidth="113.5999755859375" text="Inventory Level" />
                <TableColumn prefWidth="121.60001220703123" text="Price/Cost per Unit" />
            </columns>
        </TableView>
        <TableView layoutX="317.0" layoutY="259.0" prefHeight="138.0" prefWidth="390.0">
            <columns>
                <TableColumn prefWidth="75.0" text="Part ID" />
                <TableColumn prefWidth="75.0" text="Part Name" />
                <TableColumn prefWidth="113.5999755859375" text="Inventory Level" />
                <TableColumn prefWidth="121.60001220703123" text="Price/Cost per Unit" />
            </columns>
        </TableView>
        <Button layoutX="671.0" layoutY="214.0" mnemonicParsing="false" text="Add" />
        <Button layoutX="566.0" layoutY="404.0" mnemonicParsing="false" text="Remove Associated Part" />
    </children>

</AnchorPane>
